// mathUtils.js
function add(a, b) {
  return a + b;
}
function multiply(a, b) {
  return a * b;
}
// Export multiple functions
module.exports = {
  add,
  multiply
};
// main.js
const mathUtils = require('./mathUtils'); // or './mathUtils.js'
console.log(mathUtils.add(2, 3));       // Output: 5
console.log(mathUtils.multiply(2, 3));  // Output: 6
